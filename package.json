# go to the folder containing package.json
cd /path/to/your/project  # e.g. /workspaces/.../mspapers_92

# create package-lock.json (runs an install and writes the lockfile)
npm install

# verify lockfile was created
ls -la package-lock.json

# stage & commit
git add package-lock.json
git commit -m "chore: add package-lock.json for CI"
git push origin main
